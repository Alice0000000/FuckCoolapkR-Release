namespace dexkit.schema;

table EncodeValueByte {
    value: byte;
}

table EncodeValueShort {
    value: short;
}

table EncodeValueChar {
    value: short;
}

table EncodeValueInt {
    value: int;
}

table EncodeValueLong {
    value: long;
}

table EncodeValueFloat {
    value: float;
}

table EncodeValueDouble {
    value: double;
}

table EncodeValueString {
    value: string;
}

table EncodeValueNull {
    value: byte; // must be 0
}

table EncodeValueBoolean {
    value: bool;
}